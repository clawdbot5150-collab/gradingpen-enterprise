{
  "name": "@aivideobuddy/backend",
  "version": "1.0.0",
  "description": "Backend API server for AIVideoBuddy",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
