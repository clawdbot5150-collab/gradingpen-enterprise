{
  "name": "@robolineai/backend",
  "version": "1.0.0",
  "description": "RoboLineAI Backend API Server",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts",
    "migrate:up": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:down": "knex migrate:rollback --knexfile knexfile.ts",
    "seed": "knex seed:run --knexfile knexfile.ts",
    "typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/database.ts",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -d src/config/database.ts",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/database.ts",
    "docker:build": "docker build -t robolineai-backend .",
    "docker:run": "docker run -p 4000:4000 robolineai-backend"
  },
  "dependencies": {
    "express": "^4.18.3",
    "apollo-server-express": "^3.12.1",
    "graphql": "^16.8.1",
    "graphql-tools": "^9.0.1",
    "@graphql-tools/schema": "^10.0.3",
    "type-graphql": "^2.0.0-beta.6",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "ioredis": "^5.3.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.2.1",
    "socket.io": "^4.7.4",
    "kafkajs": "^2.2.4",
    "amqplib": "^0.10.3",
    "bull": "^4.12.2",
    "cron": "^3.1.6",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^5.0.3",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^1.0.0",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.7",
    "sharp": "^0.33.2",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^22.4.0",
    "playwright": "^1.42.1",
    "tesseract.js": "^5.0.4",
    "@tensorflow/tfjs-node": "^4.17.0",
    "openai": "^4.28.4",
    "@google-cloud/vision": "^4.3.2",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/client-textract": "^3.525.0",
    "nodemailer": "^6.9.11",
    "@sendgrid/mail": "^8.1.1",
    "twilio": "^4.23.0",
    "stripe": "^14.21.0",
    "dotenv": "^16.4.5",
    "joi": "^17.12.2",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "xml2js": "^0.6.2",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.8",
    "@types/lodash": "^4.14.202",
    "@types/xml2js": "^0.4.14",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.4.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}